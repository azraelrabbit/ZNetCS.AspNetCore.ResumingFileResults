{
  "version": "1.0.0-*",
  "authors": [ "Marcin Smó³ka" ],
  "testRunner": "mstest",

  "dependencies": {
    "ZNetCS.AspNetCore.ResumingFileResults.TestWebSite": {
      "version": "1.0.0",
      "target": "project"
    },
    "dotnet-test-mstest": "1.1.1-preview",
    "MSTest.TestFramework": "1.0.1-preview",
    "Microsoft.AspNetCore.TestHost": "1.0.0",
    "Microsoft.Net.Http.Headers": "1.0.0",
    "StyleCop.Analyzers": {
      "version": "1.0.0",
      "type": "build"
    }
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ],

      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.1",
          "type": "platform"
        }
      }
    }
  },

  "buildOptions": {
    "warningsAsErrors": false,  
    "additionalArguments": [ "/ruleset:../../CommonRuleSet.ruleset", "/additionalfile:../../stylecop.json" ]
  }
}
