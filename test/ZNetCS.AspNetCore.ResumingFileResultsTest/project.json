{
  "version": "1.0.0-*",
  "authors": [ "Marcin Smółka" ],
  "testRunner": "mstest",

  "dependencies": {
    "Microsoft.NETCore.App": {
      "version": "1.1.0",
      "type": "platform"
    },
    "ZNetCS.AspNetCore.ResumingFileResults.TestWebSite": {
      "version": "1.0.0",
      "target": "project"
    },
    "StyleCop.Analyzers": {
      "version": "1.0.0",
      "type": "build"
    },
    "Microsoft.AspNetCore.TestHost": "1.1.0",
    "Microsoft.Net.Http.Headers": "1.1.0",
    "dotnet-test-mstest": "1.1.2-preview",
    "MSTest.TestFramework": "1.0.6-preview"
  },

  "frameworks": {
    "netcoreapp1.1": {
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ],

      "dependencies": {
      }
    }
  },

  "buildOptions": {
    "warningsAsErrors": false,  
    "additionalArguments": [ "/ruleset:../../CommonRuleSet.ruleset", "/additionalfile:../../stylecop.json" ]
  }
}
